{
    "public": true,
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": { "cache-control": "max-age=604800, public" },
            "dest": "/assets/$1"
        },
        {
            "src": "/(.*\\.glb)$",
            "headers": { "cache-control": "max-age=604800, public" },
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}
